{
  "name": "RBXUtil",
  "version": "1.0.2",
  "description": "An electron app which can be used for various Roblox Utilities.",
  "main": "index.js",
  "scripts": {
    "test": "echo Test Mode && electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JuiciiYT/robloxutil.git"
  },
  "keywords": [
    "electron",
    "roblox",
    "api",
    "username"
  ],
  "build": {
    "appId": "rbxutil.id",
    "win": {
      "icon": "./assets/windows.png"
    },
    "mac": {
      "icon": "./assets/mac.icns",
      "category": "public.app-category.utilities"
    },
    "protocols": {
      "name": "rbxutil-default",
      "schemes": [
        "rbxutil"
      ]
    }
  },
  "author": "JuiciiYT",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "discord-rich-presence": "0.0.8",
    "jquery": "^3.5.1",
    "node-fetch": "^2.6.1",
    "prepend-file": "^2.0.0",
    "request": "^2.88.2",
    "roblox-user-information": "^1.0.0",
    "shelljs": "^0.8.4"
  },
  "bugs": {
    "url": "https://github.com/JuiciiYT/robloxutil/issues"
  },
  "homepage": "https://github.com/JuiciiYT/robloxutil",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-wix": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "RBXUtil"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ],
          "config": {
            "icon": "./assets/mac.icns"
          }
        },
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "language": 1033,
            "manufacturer": "RBXUtil Inc."
          }
        }
      ]
    }
  }
}
